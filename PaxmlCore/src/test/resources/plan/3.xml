<!-- make it parallelized per test -->
<plan name="xxx" >

    <execution group="G*" /> 
    
    <properties>
      selenium.browser=*firefox
      initialLanguage=nl
      snapshotBeforePageLoad=true
    </properties>
    
    <scenario path="*/Generic" group="G1">
      <factor name="f1">11</factor>
      <factor name="f2">11,12</factor>      
      <factor name="f3">3</factor>
      <factor name="f4">4</factor>
    </scenario>
    
    <scenario path="*/Generic" group="G2">
      <factor name="f1">21</factor>
      <factor name="f2">21,22</factor>          
      <factor name="f3">3</factor>
      <factor name="f4">4</factor>      
    </scenario>

    <resource>
        <include>classpath:plan/**/*.xml</include>
    </resource>
  
</plan>
