<dataSet>

    <log>Putting ds2 to ${scope} scope with flag ${flag}.</log>
 
    <data id="${scope}_ds2_1">${scope}_ds2_1_value_${flag}</data>
    <data id="${scope}_ds2_2">${scope}_ds2_2_value_${flag}</data>
    
    <log>Consts from ds2 are:</log>
    <iterate values="${util.constIds}">
        <log>${var} = ${util.getConst(var, true)}</log>
    </iterate>
    
</dataSet>
 